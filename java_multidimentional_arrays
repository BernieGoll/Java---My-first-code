public class Bern
{
    public static void main(String[] argc)
    {
        int[][] myArrays = { {1, 2, 3}, {4, 5, 6}, {7, 8, 9}};
        myArrays[1][2] = ('z');
        int i = myArrays[1][2];
        
        System.out.println(i);
    }
}
// Output: 122 (the ascii equivalent of lower case letter z)
