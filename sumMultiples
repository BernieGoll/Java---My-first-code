public class sumMultiples
{
// program sums the multiples of ranges 1-10, 20-30, 35-45
	public static void main(String[] args)
	{
		int sum1 = 0;		
		for(int i=0; i<= 10; i++)
		{
			sum1 = sum1 + i;
		}
		
		int sum2 = 0;		
		for(int i=20; i<= 30; i++)
		{
			sum2 = sum2 + i;
		}
		
		int sum3 = 0;		
		for(int i=35; i<= 45; i++)
		{
			sum3 = sum3 + i;
		}
		
		System.out.println(sum1);
		System.out.println(sum2);
		System.out.println(sum3);
	}
}

